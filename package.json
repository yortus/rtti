    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^16.*",
        "chai": "^4.3.6",
        "mocha": "^9.2.0",
        "rimraf": "^3.0.2",
        "symlink-dir": "^5.0.1",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
